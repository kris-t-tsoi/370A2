format drive2
ls /

mkfile /file1
mkfile /file2
mkfile /file3
mkfile /file4
mkfile /file5
mkfile /file6
ls /

append /file3 "I am /file3's data."
ls /
print /file3

delfile /file3
mkfile /file7
ls /

append /file7 "Now room for /file7."
ls /
print /file7

ls /

delfile /file1
ls /

mkdir /dir1
ls /
ls /dir1

mkfile /dir1/file1
mkfile /dir1/file2
mkfile /dir1/file3
mkfile /dir1/file4
mkfile /dir1/file5
mkfile /dir1/file6
mkfile /dir1/file7
mkfile /dir1/file8
